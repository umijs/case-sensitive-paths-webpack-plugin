  },
  "devDependencies": {
    "@types/jest": "^27",
    "@types/node": "^18.0.3",
    "@umijs/test": "^4.0.7",
    "css-loader": "^6.7.1",
    "father": "^4.0.0-alpha.3",
    "jest": "^27",
    "style-loader": "^3.3.1",
    "ts-node": "^10.9.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  },
  "packageManager": "pnpm@7.3.0",
